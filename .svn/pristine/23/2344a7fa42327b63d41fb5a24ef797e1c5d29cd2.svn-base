package kr.or.ddit.vo;

import java.io.Serializable;
import java.util.Date;

public class HotelReviewVO implements Serializable{
	private int hore_no; //리뷰번호
	private int mem_no; //회원번호
	private String mem_id; // 회원 아이디
	public String getMem_id() {
		return mem_id;
	}


	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}
	private int hotel_no; // 호텔번호
	private String hotel_name;
	private String hore_date; //날짜
	private String hore_writer; //작성자
	private String hore_title; //제목
	private String hore_content; // 내용
	private String hore_star; // 별점
	private String dateGu; // 해당 날짜 기간 구분
	private String hopick_date1;  // 데이트픽커 첫번째 날짜
	private String hopick_date2;  // 데이트 픽터 두번째 날짜
	
	
	
	
	


	@Override
	public String toString() {
		return "ReviewVO [hore_no=" + hore_no + ", mem_no=" 
				+ mem_no + ", mem_id=" + mem_id + ", hotel_no=" + hotel_no + ", hore_date="
				+ ", hotel_name=" + hotel_name
				+ hore_date + ", hore_writer=" + hore_writer + ", hore_title=" 
				+ hore_title + ", hore_content="
				+ hore_content + ", hore_star=" + hore_star + 
				", hopick_date1=" + hopick_date1 + ", hopick_date2=" + hopick_date2  + "]";
	}

	
	public HotelReviewVO(String hore_writer,String hore_title,
									String hore_content, String hore_star,Date hore_date){
	
		this.hore_writer = hore_writer;
		this.hore_title = hore_title;
		this.hore_content = hore_content;
		this.hore_star = hore_star;
		this.hore_date = hore_date.toString();
	}
	

	public HotelReviewVO(int hore_no, int mem_no, String mem_id,  int hotel_no, String hotel_name, String hore_date, String hore_writer, String hore_title,
			String hore_content, String hore_star, String dateGu, String hopick_date1, String hopick_date2) {
		super();
		this.hore_no = hore_no;
		this.mem_no = mem_no;
		this.mem_id = mem_id;
		this.hotel_no = hotel_no;
		this.hotel_name = hotel_name;
		this.hore_date = hore_date;
		this.hore_writer = hore_writer;
		this.hore_title = hore_title;
		this.hore_content = hore_content;
		this.hore_star = hore_star;
		this.dateGu = dateGu;
		this.hopick_date1 = hopick_date1;
		this.hopick_date2 = hopick_date2;
	}
	
	public HotelReviewVO(String hore_content, String hore_writer, String hore_date) {
		super();
		this.hore_content = hore_content;
		this.hore_writer = hore_writer;
		this.hore_date = hore_date;
	}
	
	public HotelReviewVO() {
		
	}

	public String getDateGu() {
		return dateGu;
	}


	public void setDateGu(String dateGu) {
		this.dateGu = dateGu;
	}


	public String getHotel_name() {
		return hotel_name;
	}


	public void setHotel_name(String hotel_name) {
		this.hotel_name = hotel_name;
	}


	public String getHopick_date1() {
		return hopick_date1;
	}


	public void setHopick_date1(String hopick_date1) {
		this.hopick_date1 = hopick_date1;
	}


	public String getHopick_date2() {
		return hopick_date2;
	}


	public void setHopick_date2(String hopick_date2) {
		this.hopick_date2 = hopick_date2;
	}


	public String getHore_star() {
		return hore_star;
	}
	
	public void setHore_star(String hore_star) {
		this.hore_star = hore_star;
	}
	
	public int getHore_no() {
		return hore_no;
	}
	public void setHore_no(int hore_no) {
		this.hore_no = hore_no;
	}
	public int getMem_no() {
		return mem_no;
	}
	public void setMem_no(int mem_no) {
		this.mem_no = mem_no;
	}
	public int getHotel_no() {
		return hotel_no;
	}
	public void setHotel_no(int hotel_no) {
		this.hotel_no = hotel_no;
	}
	public String getHore_date() {
		return hore_date;
	}
	public void setHore_date(String hore_date) {
		this.hore_date = hore_date;
	}
	public String getHore_writer() {
		return hore_writer;
	}
	public void setHore_writer(String hore_writer) {
		this.hore_writer = hore_writer;
	}
	public String getHore_title() {
		return hore_title;
	}
	public void setHore_title(String hore_title) {
		this.hore_title = hore_title;
	}
	public String getHore_content() {
		return hore_content;
	}
	public void setHore_content(String hore_content) {
		this.hore_content = hore_content;
	}
	
	
	
	
	


	}