package kr.or.ddit.vo;

import java.io.Serializable;

public class DogBoardVO implements Serializable{

private int dogboard_no; //게시판 넘버
private int mem_no; // 회원 넘버
private int dog_no; // 멍멍이 번호
private int com_no; // 사업자 번호
private String dogboard_title; // 게시글 제목
private String dogboard_content; // 게시글 내용
private String dogboard_date; // 날짜
private String mem_id;
private String hotel_name;

public DogBoardVO() {
	super();
}



private String dogboard_img; // 이미지
private int dogboard_good; // 좋아요 수

@Override
	public String toString() {
	return "DogboardVO [dogboard_no=" + dogboard_no
			+ ", mem_no=" + mem_no
			+ ", dog_no=" + dog_no
			+ ", com_no=" + com_no
			+ ", dogboard_title=" + dogboard_title
			+ ", dogboard_content=" + dogboard_content
			+ ", dogboard_date=" + dogboard_date
			+ ", dogboard_img=" + dogboard_img
			+ ", dogboard_good=" + dogboard_good + "]";
	}



public int getDogboard_no() {
	return dogboard_no;
}
public void setDogboard_no(int dogboard_no) {
	this.dogboard_no = dogboard_no;
}
public int getMem_no() {
	return mem_no;
}
public void setMem_no(int mem_no) {
	this.mem_no = mem_no;
}
public int getDog_no() {
	return dog_no;
}
public void setDog_no(int dog_no) {
	this.dog_no = dog_no;
}
public int getCom_no() {
	return com_no;
}
public void setCom_no(int com_no) {
	this.com_no = com_no;
}
public String getDogboard_title() {
	return dogboard_title;
}
public void setDogboard_title(String dogboard_title) {
	this.dogboard_title = dogboard_title;
}
public String getDogboard_content() {
	return dogboard_content;
}
public void setDogboard_content(String dogboard_content) {
	this.dogboard_content = dogboard_content;
}
public String getDogboard_date() {
	return dogboard_date;
}
public void setDogboard_date(String dogboard_date) {
	this.dogboard_date = dogboard_date;
}
public String getDogboard_img() {
	return dogboard_img;
}
public void setDogboard_img(String dogboard_img) {
	this.dogboard_img = dogboard_img;
}
public int getDogboard_good() {
	return dogboard_good;
}
public void setDogboard_good(int dogboard_good) {
	this.dogboard_good = dogboard_good;
}

public String getMem_id() {
	return mem_id;
}



public void setMem_id(String mem_id) {
	this.mem_id = mem_id;
}



public String getHotel_name() {
	return hotel_name;
}



public void setHotel_name(String hotel_name) {
	this.hotel_name = hotel_name;
}
	

	


}
